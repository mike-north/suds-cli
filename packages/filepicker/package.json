{
  "name": "@suds-cli/filepicker",
  "description": "File system browser component for Suds terminal UIs.",
  "version": "0.0.1-alpha.0",
  "dependencies": {
    "@suds-cli/chapstick": "workspace:*",
    "@suds-cli/help": "workspace:*",
    "@suds-cli/key": "workspace:*",
    "@suds-cli/machine": "workspace:*",
    "@suds-cli/tea": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.8.2",
    "vitest": "^4.0.16",
    "@types/node": "^24.10.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "pnpm run clean && tsc -p ./tsconfig.json",
    "check:api-report": "pnpm run generate:api-report",
    "check:eslint": "pnpm run lint",
    "clean": "rm -rf dist",
    "generate:api-report": "api-extractor run --local",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "vitest run"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
