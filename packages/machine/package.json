{
  "name": "@suds-cli/machine",
  "description": "Platform abstraction layer for Suds terminal UIs",
  "version": "0.1.0-alpha.0",
  "devDependencies": {
    "@types/archiver": "^6.0.4",
    "@types/node": "^24.10.2",
    "@types/unzipper": "^0.10.11",
    "@vitest/ui": "^4.0.16",
    "archiver": "^7.0.1",
    "clipboardy": "^4.0.0",
    "happy-dom": "^20.0.11",
    "typescript": "5.8.2",
    "unzipper": "^0.12.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.js"
      },
      "require": {
        "types": "./dist/node/index.d.cts",
        "default": "./dist/node/index.cjs"
      }
    },
    "./browser": {
      "import": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      },
      "require": {
        "types": "./dist/browser/index.d.cts",
        "default": "./dist/browser/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "peerDependencies": {
    "@xterm/xterm": "^5.0.0",
    "archiver": "^7.0.0",
    "clipboardy": "^4.0.0",
    "supports-color": "^9.0.0",
    "unzipper": "^0.12.0"
  },
  "peerDependenciesMeta": {
    "archiver": {
      "optional": true
    },
    "clipboardy": {
      "optional": true
    },
    "supports-color": {
      "optional": true
    },
    "unzipper": {
      "optional": true
    },
    "@xterm/xterm": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsup",
    "check:api-report": "api-extractor run",
    "generate:api-report": "api-extractor run --local",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:browser": "vitest run test/browser",
    "test:graceful-degradation": "vitest run -t \"\\[graceful-degradation\\]\""
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
