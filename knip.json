{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreDependencies": [
    "@vitest/coverage-v8",
    "@commitlint/cli",
    "@commitlint/types",
    "archiver",
    "clipboardy",
    "supports-color",
    "syncpack",
    "unzipper"
  ],
  "ignoreFiles": [
    "./examples/src/filetree/mock-project/**/*",
    "./examples/src/code/program.ts",
    "./packages/dsl/examples/**/*"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": ["test:graceful-degradation", "test:browser"],
  "workspaces": {
    ".": {
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    },
    "examples": {
      "entry": ["src/*/index.ts", "src/*/cli.ts"],
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    },
    "packages/boba-cli": {
      "entry": ["src/index.ts"],
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    },
    "packages/*": {
      "entry": ["src/index.ts", "test/**/*.test.ts"],
      "paths": {
        "@": ["src/index.ts"],
        "@/*": ["src/*"]
      }
    }
  }
}
